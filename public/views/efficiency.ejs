<%-include("./partials/header.ejs")%>

    <style>
        table, th, td {
        border: 1px solid gray;
        font-size: 20px;
        padding: 5px;
        }

    </style>

    <div>
        <div class="ui main text container">
            <div class="ui huge header"> Efficiency </div>
            <table>
                <tr>
                <th>Algorithm</th>
                <th>Turnaround Time</th>
