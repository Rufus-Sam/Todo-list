                <td> <b>SJF</b> </td>
                <td><%=wt_tat[2].toFixed(2)%></td>
                <td><%=wt_tat[3].toFixed(2)%></td>
                </tr>
                <tr>
                <td> <b>Priority</b> </td>
                <td><%=wt_tat[4].toFixed(2)%></td>
                <td><%=wt_tat[5].toFixed(2)%></td>
                </tr>
                <tr>
                <td> <b>Round Robin</b> </td>
                <td><%=wt_tat[6].toFixed(2)%></td>
                <td><%=wt_tat[7].toFixed(2)%></td>
                </tr>
            </table>
        </div>
    </div>

<%-include("./partials/footer.ejs")%>